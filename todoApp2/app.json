{
  "expo": {
    "name": "todoApp2",
    "slug": "todoApp2",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.todoApp"
    },
    "android": {
      "package": "todoApp"
    },
    "entryPoint": "~/AppEntry.js"
  }
}
